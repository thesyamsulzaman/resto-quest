{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "import/no-extraneous-dependencies": 0,
    "class-methods-use-this": 0,
    "prettier/prettier": ["error"]
  }
}
